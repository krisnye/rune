{
  "name": "rune-monorepo",
  "private": true,
  "version": "0.0.0",
  "description": "Monorepo for @paralleldrive/rune packages.",
  "license": "MIT",
  "packageManager": "pnpm@10",
  "scripts": {
    "build": "pnpm -r run build",
    "clean": "pnpm -r run clean",
    "dev": "sh -c 'pnpm --filter tictactoe run dev & pnpm --filter tictactoe run dev:agent; wait'",
    "dev:web": "pnpm --filter tictactoe run dev",
    "dev:agent": "pnpm --filter tictactoe run dev:agent",
    "lint": "pnpm -r run lint",
    "test": "pnpm -r run test",
    "typecheck": "pnpm -r run typecheck",
    "publish:all": "node ./scripts/publish-all-public.mjs"
  }
}
