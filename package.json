{
  "name": "rune-monorepo",
  "private": true,
  "version": "0.0.0",
  "description": "Monorepo for @paralleldrive/rune packages.",
  "license": "MIT",
  "packageManager": "pnpm@10",
  "scripts": {
    "build": "pnpm -r run build",
    "clean": "pnpm -r run clean",
    "dev": "pnpm --filter @paralleldrive/rune run build && pnpm --filter tictactoe --filter connect4 run dev",
    "dev:web": "pnpm --filter tictactoe --filter connect4 run dev",
    "lint": "pnpm -r run lint",
    "test": "pnpm -r run test",
    "typecheck": "pnpm -r run typecheck",
    "publish:all": "node ./scripts/publish-all-public.mjs"
  },
  "dependencies": {
    "@adobe/data": "0.9.27"
  }
}
